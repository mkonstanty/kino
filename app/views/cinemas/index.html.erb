<table>
	<tr>
		<th>Miasto</th>
		<th>Nazwa</th>
  </tr>

<% @cinemas.each do |cinema| %>
  <tr>
    <td><%= cinema.city %></td>
    <td><%= cinema.name %></td>
    <td><%= link_to 'Usun', cinema, :confirm => 'Czy jestes pewien?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Nowe kino', new_cinema_path %>
